{
    "name": "psrearick/tcg-collector",
    "type": "project",
    "description": "TCG Collector App",
    "keywords": ["tcg"],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "bensampo/laravel-enum": "^4.1",
        "brick/money": "^0.5.3",
        "coconutcraig/laravel-postmark": "^2.11",
        "doctrine/dbal": "^3.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "inertiajs/inertia-laravel": "^0.4.3",
        "itsgoingd/clockwork": "^5.1",
        "kalnoy/nestedset": "^6.0",
        "laravel/framework": "^8.75",
        "laravel/horizon": "^5.7",
        "laravel/jetstream": "^2.5",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "league/flysystem-aws-s3-v3": "^1.0",
        "pcrov/jsonreader": "^1.0",
        "predis/predis": "~1.0",
        "spatie/laravel-event-sourcing": "^6.0",
        "spatie/laravel-ray": "^1.29",
        "tightenco/ziggy": "^1.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "fix": [
            "php-cs-fixer fix --config=./.php-cs-fixer.dist.php"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
